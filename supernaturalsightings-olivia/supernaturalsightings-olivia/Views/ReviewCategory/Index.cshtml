@{
}
@model List<supernaturalsightings_olivia.Models.ReviewCategory>

    <h1>All Review Categories</h1>

    @if (Model.Count == 0){
        <p> No Review Categories yet</p>
    }
    else{
        <table class="table">
            <tr>
                <th>Category Name</th>
            </tr>
            @foreach (ReviewCategory category in Model){
                <tr>
                    <td>@category.ReviewType</td>
                </tr>
            }
        </table>
    }